    "testEnvironment": "jsdom",
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest-setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "modulePaths": [
      "<rootDir>/src/",
      "<rootDir>/__tests__"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/__tests__/test-utility"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
